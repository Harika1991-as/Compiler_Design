#include <stdio.h>

int main() {
    int x = 5;
    int y = 10;
    
    if (x > 3) {
        y = y + x;
    } else {
        y = y - x;
    }
    
    // Dead code
    int z = x * y; // This calculation is never used
    
    printf("Result: %d\n", y);
    
    return 0;
}
